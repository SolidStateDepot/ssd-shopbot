
#<z_cut> = -7.000
#<cutter_diameter> = [6/4]

#<x_min> = 0.25
#<x_max> = 61

#<y_min> = 2
#<y_max> = 102.2


g90
g64 p.010  ; best speed, but don't deviate from the programmed path by more than .010 inches

s12000
m3

f350

g53 g0 z0
g53 g0 x#<x_min> y#<y_min>

g53 g1 z#<z_cut>

#<y> = #<y_min>

o100 do
    g53 g1 x#<x_max>

    #<y> = [ #<y> + [#<cutter_diameter> * 0.9] ]
    o110 if [ #<y> GT #<y_max> ]
        #<y> = #<y_max>
    o110 endif

    g53 g1 y#<y>
    g53 g1 x#<x_min>

    o120 if [#<y> EQ #<y_max> ]
        o100 break
    o120 endif

    #<y> = [ #<y> + [#<cutter_diameter> * 0.9] ]
    o130 if [#<y> GT #<y_max> ]
        #<y> = #<y_max>
    o130 endif
    g53 g1 y#<y>

o100 while [#<y> LT #<y_max>]

g1 x#<x_max>
g1 y#<y_min>
g1 x#<x_min>
g1 y#<y_max>

g53 g0 z0

m2
